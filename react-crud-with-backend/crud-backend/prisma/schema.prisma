generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model customer {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique(map: "Customer_email_key")
  phone String
}

model user {
  email    String  @id @db.VarChar(70)
  password String? @db.Text
}
